{
  "manifest_version": 2,
  "name": "Monocle",
  "version": "0.2.0",
  "description": "A log analyzsis tool which is an chrome offline extension or web-based online service to help you analyze log or text files.",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon_32.png"
  },
  "background": {
    "scripts": ["start.js"]
  },
  "offline_enabled": true
}
