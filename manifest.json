{
  "manifest_version": 2,
  "name": "Monocle",
  "version": "0.0.7",
  "description": "A cross platform text analyzing tool which prvoides an chrome offline extension and web-based online service.",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon_32.png"
  },
  "background": {
    "scripts": ["start.js"]
  }
}
